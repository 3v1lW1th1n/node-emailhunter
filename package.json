{
  "name": "hunter.io",
  "version": "0.2.7",
  "description": "A simple javascript wrapper for Hunter.io API",
  "homepage": "https://b4dnewz.github.io/node-emailhunter/",
  "author": {
    "name": "b4dnewz",
    "email": "filippo@codekraft.it",
    "url": ""
  },
  "files": [
    "lib",
    "dist",
    "bin"
  ],
  "main": "dist/hunter.io.node.js",
  "bin": {
    "email-hunter": "./bin/index.js"
  },
  "keywords": [
    "email-hunter",
    "hunterio"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.2.2",
    "jest-cli": "^22.2.2",
    "nock": "^9.1.6",
    "nsp": "^3.1.0",
    "prettier": "^1.10.2",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint ./lib",
    "test": "jest --coverage",
    "prebuild": "esdoc",
    "build": "webpack",
    "updatedocs": "git subtree push --prefix docs origin gh-pages"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "camelcase": 1,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "esdoc": {
    "source": "./lib",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "repository": "b4dnewz/node-emailhunter",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "commander": "^2.14.1",
    "querystring": "^0.2.0"
  }
}
